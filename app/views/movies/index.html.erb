
<table>
  <tr>
    <h1>Movies </h1>
    <% if !user_signed_in? %>
    <div >
      <%= link_to "Sign Up to Add A Review", new_user_registration_path %>
    </div>
    <%end %>
  </tr>
<tr>
  <% @movies.each do |movie| %>
      <td>
        <%= link_to( image_tag(movie.img_url, :alt => "pic", :size => "200x300"),movie) %>
      </td>

  <% end %>
</tr>
<tr><td><%= link_to 'New Movie', new_movie_path %></td></tr>
</table>
